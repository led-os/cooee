    <!-- cheyingkun add start //解决“在其他桌面安装应用，phenix几率性异常停止运行”的问题。【i_0010424】 -->
    <!-- 智能分类进度条对话框样式 start -->
    <style name="CategoryCustomDialog" parent="@android:style/Theme.Holo.Dialog">

        <!-- 边框 start -->
        <item name="android:windowFrame">@null</item>
        <!-- 边框 end -->
        <!-- 是否浮现在activity之上 start -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 end -->
        <!-- 控制灰度的值，当为1时，界面除了我们的dialog内容是高亮显示的，dialog以外的区域是黑色的，完全看不到其他内容，系统的默认值是0.5，而已根据自己的需要调整 start -->
        <item name="android:backgroundDimAmount">0.5</item>
        <!-- 控制灰度的值，当为1时，界面除了我们的dialog内容是高亮显示的，dialog以外的区域是黑色的，完全看不到其他内容，系统的默认值是0.5，而已根据自己的需要调整 end -->
    </style>

    <style name="CategoryCustomProgressDialog" parent="@style/CategoryCustomDialog">

        <!-- 无标题 start -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 end -->
        <!-- 设置自定义布局的背景透明 start -->
        <item name="android:background">@android:color/transparent</item>
        <!-- 设置自定义布局的背景透明 end -->
        <!-- 设置window背景透明 start -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 设置window背景透明 end -->
    </style>
    <!-- 智能分类进度条对话框 end -->
