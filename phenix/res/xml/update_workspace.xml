<?xml version="1.0" encoding="utf-8"?>
<!-- 此文件用于升级数据库  数据库version高于9 有效      同时具备初始化数据库功能 -->
<favorites xmlns:launcher="http://schemas.android.com/apk/res-auto/com.cooee.phenix" >

    <!-- Update the db with new hotseat items.  Note that we reference the browser's original package name. -->
    <!-- Hotseat (We use the screen as the position of the item in the hotseat) -->
    <favorite
        launcher:cellX="0"
        launcher:cellY="0"
        launcher:className="com.android.dialer.DialtactsActivity"
        launcher:container="-101"
        launcher:packageName="com.android.dialer"
        launcher:screen="0" />
    <favorite
        launcher:cellX="1"
        launcher:cellY="0"
        launcher:className="com.android.contacts.activities.PeopleActivity"
        launcher:container="-101"
        launcher:packageName="com.android.contacts"
        launcher:screen="1" />
    <favorite
        launcher:cellX="3"
        launcher:cellY="0"
        launcher:className="com.android.mms.ui.ConversationList"
        launcher:container="-101"
        launcher:packageName="com.android.mms"
        launcher:screen="3" />
    <favorite
        launcher:cellX="4"
        launcher:cellY="0"
        launcher:className="com.android.browser.BrowserActivity"
        launcher:container="-101"
        launcher:packageName="com.android.browser"
        launcher:screen="4" />

</favorites>