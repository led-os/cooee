<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.cooee.widgetnative.P3in1"
    android:versionCode="39691"
    android:versionName="2017.03.13.39691" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 显示系统窗口 -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <!-- 允许程序在手机屏幕关闭后后台进程仍然运行 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!-- 允许程序开机自动运行 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <!-- shell 注册权限开始 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <!-- shell 注册权限结束 -->


    <!-- kpush start -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.CREATE_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <!-- kpush end -->

    <application
        android:allowBackup="true"
        android:allowClearUserData="false"
        android:icon="@drawable/widget_preview_image"
        android:label="@string/app_name"
        android:persistent="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cooee.widgetnative.P3in1.Photo.activity.PickPicDialogActivity"
            android:excludeFromRecents="true"
            android:exported="true"
            android:process=":remote"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!-- 广告 start -->
        <!-- <activity android:name="com.kmob.kmobsdk.InAppWebView" /> -->
        <!-- <activity -->
        <!-- android:name="com.kmob.kmobsdk.AdActivity" -->
        <!--
             android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" 
			/>
        -->


        <!-- <service -->
        <!-- android:name="com.kmob.kmobsdk.KmobService" -->
        <!-- android:exported="true" -->
        <!-- android:process="com.kmob.dl.process" /> -->


        <!-- <meta-data -->
        <!-- android:name="KMobAd_APP_ID" -->
        <!-- android:value="502" > -->
        <!-- </meta-data> -->
        <!-- 广告 end -->

        <meta-data
            android:name="preInitialize"
            android:value="false" />
        <meta-data
            android:name="max_instance_count"
            android:value="1" />
        <meta-data
            android:name="spanX"
            android:value="2" />
        <meta-data
            android:name="spanY"
            android:value="1" />
        <meta-data
            android:name="widget_width"
            android:value="236.00" />
        <meta-data
            android:name="widget_height"
            android:value="236.00" />
        <meta-data
            android:name="useTheme"
            android:value="true" />

        <!-- receiver的 android:name指向的是widget的请求处理器或者说请求接收者 -->
        <receiver
            android:name="com.cooee.widgetnative.P3in1.base.WidgetProvider"
            android:label="@string/app_name"
            android:process=":remote" >
            <intent-filter>
                <action android:name="com.cooee.widgetnative.P3in1.click.photo_layout" />
                <!-- widget默认的事件action -->
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
                <!-- 用于服务被杀死后重启服务 -->
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.intent.action.PACKAGE_RESTARTED" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MEDIA_MOUNTED" />

                <data android:scheme="file" />
            </intent-filter>
            <intent-filter android:priority="2147483643" >
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.intent.action.PHONE_STATE" />
                <action android:name="android.intent.action.SIM_STATE_CHANGED" />
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
            <!-- widget元数据,name是写死的，resource指的是widget的配置文件 -->
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/widget_info" />
        </receiver>

        <service
            android:name="com.cooee.widgetnative.P3in1.base.WidgetService"
            android:process=":remote" >
            <intent-filter android:priority="2147483640" >

                <!-- 用于服务被杀死后重启服务 -->
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.intent.action.PACKAGE_RESTARTED" />
            </intent-filter>
        </service>
        <service
            android:name="com.kpsh.sdk.KpshService"
            android:process="com.kpsh.kpshservice" >
            <intent-filter android:priority="250" >
                <action android:name="com.kpsh.sdk.action.KpshService" />
            </intent-filter>
        </service>
        <service
            android:name="com.cooee.shell.shell.SdkServiceHullV5"
            android:process="com.platform.shell.sdk" >
            <intent-filter android:priority="250" >
                <action android:name="com.cooee.shell.action.SdkServiceV5" />
            </intent-filter>
        </service>
        <service
            android:name="com.cooee.shell.pay.PayServiceHullV5"
            android:process="com.platform.shell.pay" >
            <intent-filter android:priority="250" >
                <action android:name="com.cooee.shell.action.PayServiceV5" />
            </intent-filter>
        </service>

        <receiver
            android:name="com.cooee.widgetnative.P3in1.base.WidgetRestartReceiver"
            android:process=":remote" >
            <intent-filter>
                <action android:name="com.cooee.phenix.onResume.UpdateWidgetView" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.cooee.shell.shell.SdkReceiverHullV5" >
            <intent-filter android:priority="2147483647" >
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.intent.action.PHONE_STATE" />
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.intent.action.ACTION_SHUTDOWN" />
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.kpsh.sdk.KpshReceiver" >
            <intent-filter android:priority="2147483647" >
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />

                <data android:scheme="package" />
            </intent-filter>
            <intent-filter android:priority="2147483647" >
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.intent.action.PHONE_STATE" />
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.intent.action.USER_CLEARNOTIFY" />
                <action android:name="android.intent.action.SEND_TO_KPSH" />
                <action android:name="android.intent.action.CHECK_RECEIVER" />
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.cooee.shell.shell.SdkActivityHullV5"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@android:color/transparent" >
        </activity>
        <activity
            android:name="com.cooee.shell.pay.PayActivityHullV5"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="com.cooee.shell.download.DownloaderActivityHullV5"
            android:configChanges="orientation|keyboardHidden|screenSize" >
        </activity>
        <activity
            android:name="com.kpsh.sdk.KpshActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:taskAffinity="com.kpsh.sdk.callScreen"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
    </application>

</manifest>