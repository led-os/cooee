<resources>

    <!-- Base application theme, dependent on API level. This theme is replaced -->
    <!-- by AppBaseTheme from res/values-vXX/styles.xml on newer devices. -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!-- Theme customizations available in newer API levels can go in -->
        <!-- res/values-vXX/styles.xml, while customizations related to -->
        <!-- backward-compatibility can go here. -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="KusoSearchMainContentStyle">
        <item name="android:paddingLeft">7dip</item>
        <item name="android:paddingRight">7dip</item>
    </style>

    <style name="KusoSearchMainEditStyle">
        <item name="android:paddingLeft">58dip</item>
        <item name="android:paddingRight">55dip</item>
    </style>

    <style name="KusoSearchMainOpenPopStyle">
        <item name="android:layout_alignLeft">@id/kuso_search_edit</item>
    </style>

    <style name="KusoSearchMainCurrentLogoStyle">
        <item name="android:layout_marginLeft">10dip</item>
    </style>

    <style name="KusoSearchMainLocalSerachCurrentLogoStyle">
        <item name="android:layout_marginLeft">14dip</item>
    </style>

    <style name="KusoSearchMainEngineTriangleStyle">
        <item name="android:layout_toRightOf">@id/kuso_current_logo</item>
    </style>

    <style name="KusoSearchMainIconfontSousuoButtonContentStyle">
        <item name="android:layout_alignRight">@id/kuso_search_edit</item>
    </style>

    <style name="KusoSearchMainIconfontSousuoButtonLineStyle">
        <item name="android:layout_marginRight">6dip</item>
    </style>

    <style name="KusoSearchMainIconfontSousuoButtonStyle">
        <item name="android:layout_marginRight">22dip</item>
    </style>

    <style name="KusoSearchMainHistoryStyle">
        <item name="android:layout_marginLeft">13dip</item>
    </style>

    <style name="KusoSearchMainHistoryRecordClearContentStyle">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">8dip</item>
    </style>

    <style name="KusoSearchMainHistoryRecordClearIconStyle">
        <item name="android:layout_marginRight">10dip</item>
    </style>

</resources>