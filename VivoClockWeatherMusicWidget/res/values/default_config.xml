<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- clockcalendar -->
	<!-- 是否显示时钟日历 -->
	<bool name="show_clockView">true</bool>
	<!-- 点击时间组件后进入的默认apk的包名，不填则根据默认规则进入时钟 -->
	<string name="default_clock_package"></string>
	<!-- weather -->
	<!-- 是否显示天气 -->
	<bool name="show_weatherView">true</bool>
	<!-- 点击天气组件后默认进入的天气客户端apk的包名，不填则根据默认对接2345天气客户端(com.tianqiwhite),我们的天气客户端包名是com.cooee.widget.samweatherclock -->
	<string name="default_weather_package">com.cooee.widget.samweatherclock</string>
	<!-- 温度默认显示名称 -->
	<integer name="default_tempC">0</integer>
	<!-- 城市默认显示名称 -->
	<string name="city_name">----</string>
	<!-- music -->
	<!-- 是否显示音乐 -->
	<bool name="show_musicView">true</bool>
</resources>
